{
  "name": "neodoc",
  "version": "2.0.2",
  "description": "Beautiful, handcrafted command line interfaces",
  "main": "index.js",
  "bin": {
    "neodoc": "cli.js"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "minify": "uglifyjs --compress --mangle --output=index.js index.js",
    "bundle": "spago bundle-module && npm run minify",
    "prepublishOnly": "npm test && npm run bundle",
    "example": "spago run --main=Example",
    "build": "spago build",
    "test": "spago test",
    "watch": "spago run --watch",
    "deps": "spago install",
    "clean": "git stash && git clean -fdX",
    "prof": "spago build && ./scripts/prof"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixschl/neodoc.git"
  },
  "keywords": [
    "docopt",
    "neodoc",
    "cli",
    "parser",
    "parsing",
    "argument",
    "args",
    "option",
    "command"
  ],
  "author": "Felix Schlitter <felixschlitter@gmail.com>",
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/felixschl/neodoc/issues"
  },
  "homepage": "https://github.com/felixschl/neodoc#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "js-schema": "^1.0.1",
    "lodash": "^4.12.0",
    "mocha": "^2.4.5",
    "spago": "^0.13.1",
    "string-argv": "0.0.2",
    "uglify-js": "^3.7.6"
  },
  "dependencies": {
    "ansi-regex": "^2.0.0"
  }
}
